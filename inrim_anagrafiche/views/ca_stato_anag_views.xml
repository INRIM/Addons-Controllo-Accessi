<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="ca_stato_anag_view_tree" model="ir.ui.view">
        <field name="name">ca.stato.anag.view.tree</field>
        <field name="model">ca.stato_anag</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="name"/>
                <field name="description"/>
                <field name="date_start"/>
                <field name="date_end"/>
            </tree>
        </field>
    </record>

    <record id="ca_stato_anag_action" model="ir.actions.act_window">
        <field name="name">Record Loading States</field>
        <field name="res_model">ca.stato_anag</field>
        <field name="view_mode">tree</field>
    </record>
</odoo>