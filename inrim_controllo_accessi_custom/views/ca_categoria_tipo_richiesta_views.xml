<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="ca_categoria_tipo_richiesta_view_tree" model="ir.ui.view">
        <field name="name">ca.categoria.tipo.richiesta.view.tree</field>
        <field name="model">ca.categoria_tipo_richiesta</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="name"/>
                <field name="ca_categoria_richiesta"/>
                <field name="description"/>
                <field name="date_start"/>
                <field name="date_end"/>
            </tree>
        </field>
    </record>

    <record id="ca_categoria_tipo_richiesta_action" model="ir.actions.act_window">
        <field name="name">Type of Access Request</field>
        <field name="res_model">ca.categoria_tipo_richiesta</field>
        <field name="view_mode">tree</field>
    </record>
</odoo>